{
  "name": "www-vorote-ch",
  "version": "0.0.1",
  "private": true,
  "packageManager": "pnpm@9.10.0",
  "scripts": {
    "dev": "tinacms dev -c \"next dev --turbopack\"",
    "build": "tinacms build --partial-reindex && next build",
    "deploy": "opennextjs-cloudflare build && opennextjs-cloudflare deploy",
    "preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview",
    "upload": "opennextjs-cloudflare build && opennextjs-cloudflare upload",
    "check": "opennextjs-cloudflare build && tsc && wrangler deploy --dry-run",
    "start": "next start",
    "lint": "biome lint",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv cloudflare-env.d.ts",
    "test:scheduler": "npx tsx components/tools/scheduler/scheduler.test.ts"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@opennextjs/cloudflare": "1.14.6",
    "@svgr/webpack": "^8.1.0",
    "@tailwindcss/typography": "^0.5.16",
    "@tinacms/cli": "^2.1.2",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^22.13.10",
    "@types/react": "^18.3.18",
    "@types/styled-components": "^5.1.34",
    "autoprefixer": "^10.4.21",
    "baseline-browser-mapping": "^2.9.19",
    "postcss": "^8.5.3",
    "postcss-import": "^16.1.0",
    "postcss-nesting": "^13.0.1",
    "typescript": "^5.8.2",
    "wrangler": "^4.59.1"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.1",
    "@radix-ui/react-avatar": "^1.1.3",
    "@radix-ui/react-slot": "^1.1.2",
    "@tailwindcss/postcss": "^4.0.17",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^3.6.0",
    "lucide-react": "^0.484.0",
    "mermaid": "^11.10.0",
    "motion": "^12.6.3",
    "next": "16.0.10",
    "next-themes": "^0.4.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.5.0",
    "react-player": "^2.16.0",
    "react-use-measure": "^2.1.7",
    "shiki": "^3.2.2",
    "styled-jsx": "^5.1.6",
    "tailwind-merge": "^2.6.0",
    "tailwindcss": "^4.0.17",
    "tinacms": "^3.3.2",
    "tw-animate-css": "^1.2.4",
    "usehooks-ts": "^3.1.1"
  },
  "pnpm": {
    "overrides": {
      "esbuild": "0.25.2",
      "@opennextjs/aws>esbuild": "0.25.0",
      "jsonpath-plus": "10.3.0",
      "mermaid": "11.10.0",
      "react-router": "7.5.2"
    }
  }
}
