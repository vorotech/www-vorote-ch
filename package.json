{
  "name": "www-vorote-ch",
  "version": "0.0.1",
  "private": true,
  "packageManager": "pnpm@9.10.0",
  "scripts": {
    "dev": "tinacms dev -c \"next dev --turbopack\"",
    "dev:build": "next build",
    "build": "tinacms build && next build",
    "build:local": "NODE_OPTIONS=\"--max-old-space-size=4096\" tinacms build --local --skip-indexing --skip-cloud-checks && next build",
    "build:ci": "NODE_OPTIONS=\"--max-old-space-size=4096\" tinacms build --skip-indexing --skip-cloud-checks && next build",
    "deploy": "opennextjs-cloudflare build && opennextjs-cloudflare deploy",
    "preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview",
    "upload": "opennextjs-cloudflare build && opennextjs-cloudflare upload",
    "check": "opennextjs-cloudflare build && tsc && wrangler deploy --dry-run",
    "start": "tinacms build && next start",
    "lint": "biome lint",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv cloudflare-env.d.ts"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@cloudflare/workers-types": "^4.20250423.0",
    "@opennextjs/cloudflare": "1.3.0",
    "@svgr/webpack": "^8.1.0",
    "@tailwindcss/typography": "^0.5.16",
    "@tinacms/cli": "^1.9.8",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^22.13.10",
    "@types/react": "^18.3.18",
    "@types/styled-components": "^5.1.34",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.3",
    "postcss-import": "^16.1.0",
    "postcss-nesting": "^13.0.1",
    "typescript": "^5.8.2",
    "wrangler": "^4.12.0"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.1",
    "@radix-ui/react-avatar": "^1.1.3",
    "@radix-ui/react-slot": "^1.1.2",
    "@tailwindcss/postcss": "^4.0.17",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^3.6.0",
    "lucide-react": "^0.484.0",
    "mermaid": "^11.6.0",
    "motion": "^12.6.3",
    "next": "15.3.3",
    "next-themes": "^0.4.6",
    "react": "^18",
    "react-dom": "^18",
    "react-icons": "^5.5.0",
    "react-player": "^2.16.0",
    "react-use-measure": "^2.1.7",
    "shiki": "^3.2.2",
    "styled-jsx": "^5.1.6",
    "tailwind-merge": "^2.6.0",
    "tailwindcss": "^4.0.17",
    "tinacms": "^2.7.8",
    "tw-animate-css": "^1.2.4",
    "usehooks-ts": "^3.1.1"
  },
  "pnpm": {
    "overrides": {
      "esbuild": "0.25.2",
      "@opennextjs/aws>esbuild": "0.25.0",
      "jsonpath-plus": "10.3.0",
      "mermaid": "11.6.0",
      "react-router": "7.5.2"
    }
  }
}
